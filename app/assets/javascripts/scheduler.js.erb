$(document).ready(function(){
  $(".create-appointment").on("click", function(){
    date = $(this).data("date");
    loadAppointmentForm(date);
  });
});

function loadAppointmentForm(date){
  $.get( Routes.new_appointments_path(), { date: date })
  .done(function(data){
    showAppointmentForm(data);
  });
};

